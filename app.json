{
  "expo": {
    "name": "Baru App",
    "slug": "baru-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "fonts": [
      "assets/BMJUA_ttf.ttf",
      "assets/assets/fonts/Hakgyoansim_BoardmarkerR.ttf"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.baru.app"
    },
    "android": {
      "package": "com.baru.app"
    },
    "plugins": [
      "expo-font"
    ]
  }
}
